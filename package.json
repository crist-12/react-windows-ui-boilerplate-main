{
  "name": "kassa-app",
  "version": "1.0.0",
  "private": true,
  "description": "Recarga Veloz",
  "keywords": [
    "react",
    "fluentui",
    "windows",
    "user-interface"
  ],
  "homepage": "https://virtualvivek.github.io/react-windows-ui/",
  "bugs": {
    "url": "https://github.com/virtualvivek/react-windows-ui/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Recarga Veloz",
    "email": "rv@email.com"
  },
  "main": "public/electron.js",
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@hookstate/core": "^3.0.13",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@mui/material": "^5.3.0",
    "@mui/styles": "^5.3.0",
    "electron-dev": "^1.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "mui-datatables": "^4.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-windows-ui": "^4.0.8",
    "web-vitals": "^0.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virtualvivek/react-windows-ui.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "eject": "react-scripts eject",
    "electron-dev": "electron .",
    "dist": "electron-builder",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.kassarv.app",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": true,
      "allowToChangeInstallationDirectory": false
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron": "^16.0.7"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "kassa_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
