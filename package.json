{
  "name": "kassa-app",
  "version": "1.0.0",
  "private": true,
  "description": "Recarga Veloz",
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "Recarga Veloz",
    "email": "rv@email.com"
  },
  "productName": "Kassa",
  "main": "electron/main.js",
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@hookstate/core": "^3.0.13",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@mui/material": "^5.3.0",
    "@mui/styles": "^5.3.0",
    "axios": "^0.25.0",
    "electron-dev": "^1.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "env-cmd": "^10.1.0",
    "mui-datatables": "^4.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-windows-ui": "^4.0.8",
    "web-vitals": "^0.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virtualvivek/react-windows-ui.git"
  },
  "scripts": {
    "start": "env-cmd react-scripts start",
    "build": "env-cmd react-scripts build",
    "predeploy": "npm run build",
    "eject": "react-scripts eject",
    "electron-dev2": "set REACT_APP_ELECTRON_START_URL= http://localhost:3000/login && electron .",
    "electron-dev": "electron .",
    "dist": "electron-builder",
    "package2": "electron-forge package",
    "make": "electron-forge make",
    "build-electron": "mkdir build/src && robocopy electron build/electron /S & robocopy src/shared build/src/shared /S",
    "package": "electron-builder build --win -c.extraMetadata.main=build/electron/main.js --publish never"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "electron-cra-example",
      "owner": "johndyer24"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5"
  }
}
